
<div class="browse_btns">
	<div class="by-muscle"><h3>Select Exercise by Muscle</h3></div>
	<div class="by-category"><h3>Select Exercise by Category</h3></div>
</div>

<div class="new-activity-form">
	<%= form_for @exercise_activity do |f| %>
		
		<p><%= f.label :weight %></p>
		<p><%= f.text_field :weight %></p>
		
		<p><%= f.label :reps %></p>
		<p><%= f.text_field :reps %></p>

		<p><%= f.submit "Complete Set" %></p>
		<% end %>
</div>

